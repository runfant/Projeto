        .wrapper-header-bt {
            position: relative;
            padding-top: 10px;

            .header-logo {
                padding: 8px 0 8px;
            }

            .header-panel-bt {
                @include flexbox();
                @include justify-content(space-between);
                @include align-items(flex-start);
            }

            .icon-search {
                svg {
                    top: 1px;
                }
            }
        }

        .left-header {
            padding-right: 15px;
            width: 140px;
        }

        .right-header {
            @include flexbox();
            @include justify-content(flex-end);
            @include align-items(center);
            margin-top: 1px;

            > * {
                width: 50px;
                height: 50px;
                @include justify-content(flex-end);
            }

            .hamburger-icon {
                position: relative;
                top: 1px;
            }

            .icon-user {
                position: relative;
                top: -1px;
                padding: 3px 0;

                svg {
                    width: 22px;
                }
            }

            .wishlist-icon {
                padding-right: 3px;
            }

            .cart-icon {
                padding-right: 2px;
                position: relative;
                top: -1px;

                .cartCount {
                    right: -10px;
                    top: 6px;
                }
            }

            .nav-search {
                .search-form {
                    @include flexbox();
                    @include align-items(center);
                    position: absolute;
                    width: 100%;
                    z-index: 3;
                    height: 100%;
                    background-color: #f8f8f8;
                    visibility: hidden;
                    @include scale(0);
                    @include opacity(0);
                    @include transition(transform .5s ease);
                }

                .search-bar {
                    @include flexbox();
                    width: 1170px;
                    max-width: 82%;
                    margin: auto;
                    top: 0;
                    height: 100%;

                    .icon-search {
                        padding-right: 0;
                        @include justify-content(flex-end);
                        width: 52px;

                        svg {
                            width: 18px;
                            height: 18px;
                        }
                    }
                }

                .input-group-field {
                    font-size: $font_size + 4;
                    @include calc(width, '100% - 52px');
                    text-align: center;
                    padding-left: 64px;
                    line-height: $font_size + 11;
                }

                .close-search {
                    left: initial;
                    @include calc(right, '1.5% - 4px');
                    @include calc(top, '50% + 1px');
                    @include translateY(-50%);
                }
            }
        }

        .wrapper-navigation {
            margin-top: 4px;
        }

        .site-nav {
            .menu-lv-1 {
                > a {
                    padding-bottom: 22px;
                }

                .icon-label {
                    top: -4px;
                }
            }
        }
