            }

            &.abs_right {
                @include calc(right, '(100% - 950px)/2');
            }
        }


        .banner-large-img {
            .inner {
                &.abs_left {
                    @include calc(left, '(100% - 950px)/2');
                }

                &.abs_right {
                    @include calc(right, '(100% - 950px)/2');
                }
            }
        }
    }

    .home-brands-block,
    .brands-page .logo-brands {
        .brands-slider {
            &:not(.slick-slider) {
                .item {
                    &:nth-child(-n + 4) {
                        display: block;
                    }
                }
            }
        }
    }


    .featured-products {
        &.has-banner {
            .column-banner {
                padding-right: 10px;
            }

            .column-product {
                @include calc(padding-right, '(100% - 950px)/2');

                .column-product-inner {
                    @include calc(max-width, '970px/10 * 6 - 20px');
                }
            }

            .flex-row-reverse {
                .column-banner {
                    padding-right: 0;
                    padding-left: 10px;
                }

                .column-product {
                    @include calc(padding-left, '(100% - 950px)/2');
                    padding-right: 10px;
                }
            }
        }
    }


    .home-collections {
        .collections-slider {
            padding-left: 20px;
            @include calc(margin-left, '(-100vw + 970px)/2 - 10px');
