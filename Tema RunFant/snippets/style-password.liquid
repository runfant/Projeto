<!--layout2-->
<style>
  .password-newsletter-success{
    padding:7px 10px;
    border:1px solid #0dbb68;
    background: #eefdf5;
    color:green;
    margin-bottom:10px!important;
  }
  .header-password{
    text-align:center;
  }

  .header-password .header-top{
    text-transform: uppercase;
    letter-spacing: 0.05em;
    font-weight: 600;
    background-color: #f5f5f5;
    padding:7px 0px 5px;
  }
  .header-password .header-logo{
    padding:20px 0px;
    text-align: center;
  }
  .header-password .header-logo a{
    transition: 0s;
    color:{{section.settings.brand_text_color}};
    text-decoration: none;
    display:inline-block;
  }
  .header-password .header-logo img{
    max-height:{{section.settings.max_height_logo_pc}};
  }
  .header-password .header-logo img:last-child{
    display:none;
  }
  .header-password .header-logo img:first-child{
    display:block;
  }
  .not_img{
    height:880px;
    line-height:1500px;
  }
  .password-content .banner{
    position: relative;
    color:{{section.settings.text_color}};
  }
  .password-content .banner .image {
    position: relative;
  }
  .password-content .banner .image .text{
    display:none;
    text-align: center;
  }
  .password-content .banner img{
    display:block;
    margin:0 auto;
  }
  .password-content .content,.password-content .banner .image .text{
    position:absolute;
    top:50%;
    left:50%;
    -webkit-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    max-width:650px;

  }
  .password-content  .heading{
    font-size:{{section.settings.heading_size}}px;
    font-weight: 700;
    line-height: {{section.settings.heading_size | plus: 15 }}px;
    margin-bottom:30px;
    text-transform: uppercase;
    letter-spacing: 0.02em;
    text-shadow:0px 1px 2px rgba(0,0,0,0.1);
    position: relative;
  }
  .password-content  .heading span.demo{
    position: relative;
    display: inline-block;
  }
  .password-content  .heading span.demo:before{
    content:'';
    display:block;
    position:absolute;
    width: 100%;
    height:1px;
    background-color: {{section.settings.text_color}};
    bottom:6px;
    left:0;
  }
  .password-content p.subtitle{
    font-size:{{section.settings.subtitle_size}}px;
    line-height: {{section.settings.subtitle_size | plus: 8 }}px;
    margin:0px 0px 38px;
    letter-spacing: 0.05em;
    text-shadow:0px 1px 2px rgba(0,0,0,0.1);
    text-transform: uppercase;
  }
  #countdown{
    display: -webkit-flex;
    display: -ms-flex;
    display: flex;
    -webkit-justify-content: center;
    -moz-justify-content: center;
    -ms-justify-content: center;
    justify-content: center;
    margin-bottom:60px;
  }
  .clock-item {
    display: block;
    margin:0px 7px;
    text-transform: uppercase;
    text-align: center;
    font-weight: 600;
    letter-spacing: 0.05em;
  }
  .clock-item .num {
    display: -webkit-flex;
    display: -ms-flex;
    display: flex;
  }
  .clock-item span{
    display:block;
    padding-top:26px;
    width: 60px;
    height: 75px;
    background-color: #fff;
    color:#232323;
    font-size: 30px;
    margin:3px;
    box-shadow: 0px 1px 2px rgba(0,0,0,.1);
    border-radius:3px;
    position: relative;
  }
  .clock-item span:before,.clock-item span:after{
    content:'';
    display: block;
    width:100%;
    height:1px;
    background-color: #e0e0e0;
    position: absolute;
    left:0;
  }
  .clock-item span:before{
    bottom:2px;
  }
  .clock-item span:after{
    bottom:5px;
  }
  .clock-item p{
    font-size: 14px;
    line-height: 20px;
    color:{{section.settings.text_color}};
    text-shadow: 0px 1px 2px rgba(0,0,0,.1);
  }
  .access-area{
    width:100%;
    max-width: 425px;
    margin:0 auto;
  }
  .access-area label{
    display: block;
    text-transform: uppercase;
    font-weight: 500;
    letter-spacing: 0.05em;
    margin-bottom:16px;
    color:{{section.settings.text_color}};
  }
  .access-area #password{
    width:100%;
    height:40px;
    border:0px;
    text-align: center;
    margin-bottom:15px;
    border-radius:3px
  }
  .access-area .btn{
    width:100%;
    border-radius:3px;
  }
  .newsleter-area{
    text-align: center;
    padding:64px 10px 100px;
    background-color:{{section.settings.newsletter_background_color}}
  }
  .newsleter-area .text{
    display: inline-block;
    text-transform: uppercase;
    padding-bottom:40px;
  }
  .newsleter-area h2.title{
    font-size: {{section.settings.newsletter_title_size}}px;
    color:{{section.settings.newsletter_title_color}};
    font-weight: 700;
    margin-bottom:14px;
  }
  .newsleter-area p.subtitle{
    font-size: {{section.settings.newsletter_subtitle_size}}px;
    color:{{section.settings.newsletter_subtitle_color}};
    font-weight: 400;
    margin:0px;
  }
  .newsleter-area p.subtitle span.demo{
    text-decoration: underline;
  }
  #contact_form{
    display:block;
    margin:0 auto;
    width:100%;
    max-width: 425px;
  }

  #contact_form .input-group-field{
    width:100%;
    border:1px solid #cbcbcb;
    border-radius:3px;
    text-align: center;
    margin-bottom:15px;
    background-color: #fff;
  }
  #contact_form .btn{
    width:100%;
    border-radius:3px;
    background: {{settings.btn_1_bg_color}};
    color: {{settings.btn_1_text_color}};
    border-color: {{settings.btn_1_border_color}};
  }
  #contact_form .btn:hover{
    background: {{settings.btn_2_bg_color}};
    color: {{settings.btn_2_text_color}};
    border-color: {{settings.btn_2_border_color}};
  }

  footer{
    padding:30px 0px 40px;
    text-align:center;
  }
  address, address a{
    color:{{settings.body_color}};
    margin:0px;
  }

  @media (max-width:1024px){
    .password-content .content{
      position: static;
      -webkit-transform: translate(0,0);
      -ms-transform: translate(0,0);
      transform: translate(0,0);
      color:#232323;
      padding:50px 10px;
      margin: 0 auto;
    }
      .password-content .content .text{
        display: none;
      }
    .access-area #password{
      border: 1px solid #cbcbcb;
    }
    .clock-item span{
      border: 1px solid #cbcbcb;
    }
    .clock-item p{
      color:#232323;
    }
    .password-content .banner .image .text{
      display:block;
    }
  }
@media (max-width:767px){
  .header-password .header-logo{
    padding:10px 0px;
  }
  .header-password .header-logo img{
    max-height:{{section.settings.max_height_logo_mb}};
  }
  .password-content .content .text{
    display: none;
  }
  .password-content .banner .image .text{
    width:90%;
  }
  .password-content .heading,.newsleter-area h2.title{
    font-size: 20px!important;
        line-height: 32px;
        margin-bottom: 8px;
  }
  .password-content p.subtitle{
    margin-bottom: 0px;
  }
  #countdown{
    -webkit-flex-wrap: wrap;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    margin-bottom:0px;
  }
  .clock-item .num span{
    font-size: 18px;
    width:30px;
    height:37px;
    padding-top:10px;

  }
}
</style>
{% endif %}
