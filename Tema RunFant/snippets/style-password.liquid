{% if settings.password_layout == 'default'%}
<style>
  .password-newsletter-success{
    padding:7px 10px;
    border:1px solid #0dbb68;
    background: #eefdf5;
    color:green;
    margin-bottom:10px!important;
  }
.password-wrapper{
  position: relative;
      width: 100%;
}
.header-password{
  display: -webkit-flex;
  display: -ms-flex;
  display: flex;
  -webkit-justify-content: space-between;
  -ms-justify-content: space-between;
  justify-content: space-between;
  padding:30px 60px;
  position: absolute;
  top:0;
  left:0;
  width:100%;
  background: transparent;
  z-index: 2;
}
.header-password .header-logo{
  max-width: 50%;
  padding-right:15px;
}
.header-password .header-logo a{
  transition: 0s;
  color:{{section.settings.brand_text_color}};
  text-decoration: none;
}
.header-password .header-logo img{
  max-height:{{section.settings.max_height_logo_pc}};
}
.header-password .header-logo img:last-child{
  display:none;
}
.header-password .header-logo img:first-child{
  display:block;
}
.access-area{
  display: -webkit-flex;
  display: -ms-flex;
  display: flex;
  -webkit-align-items: center;
  -ms-align-items: center;
  align-items: center;
  color:{{section.settings.access_lable_color}};
  cursor: pointer;
  max-width:50%;
  padding-left:15px;
  position:relative;
}

.password-content{
  display: -webkit-flex;
  display: -ms-flex;
  display: flex;
  background-color:{{section.settings.backbround_color}};
}
.password-content .col{
  max-width:50%;
  flex:1;
  padding:0;
}
.password-content .col-left{
  position:relative;
}
.password-content .col-right img{
  display: block;
  margin-right:0;
  margin-left: auto;
}
.password-content .col-right .not_img{
  height:100vh;
  width:100%;
  line-height: 100vh;
}
.password-content .content{
  color:{{section.settings.text_color}};
  text-align:left;
  position:absolute;
  top:50%;
  left:50%;
  -webkit-transform: translate(-50%,-50%);
  -ms-transform: translate(-50%,-50%);
  transform: translate(-50%,-50%);
  max-width:390px;

}
.password-content .content .heading{
  font-size:{{section.settings.heading_size}}px;
  font-weight: 500;
  line-height: {{section.settings.heading_size | plus: 10 }}px;
  margin-bottom:30px;
}
.password-content .content p.subtitle{
  font-size:{{section.settings.subtitle_size}}px;
  line-height: {{section.settings.subtitle_size | plus: 8 }}px;
  margin:0px 0px 38px;
}
.password-content .content #contact_form{
  width:100%;
  max-width:380px;
}
.password-content .content .input-group-field{
  width:100%;
  border:1px solid #cbcbcb;
  margin-bottom:15px;
  height:40px;
}
.password-content .content .btn{
  width:100%;
  font-weight: 600;
  margin-bottom:45px;
}
footer{
  position:absolute;
  left:0;
  bottom:0;
  padding:60px;
}
address, address a{
  color:{{section.settings.text_color}};
  margin:0px;
}
#login_form{
  display: none;
  position:absolute;
  right:0;
  top:100%;
  background: #fff;
  padding:20px 30px;
  -webkit-box-shadow: 0 0 0 1px rgba(0,0,0,0.06), 0 2px 2px 0 rgba(0,0,0,0.1);
-moz-box-shadow: 0 0 0 1px rgba(0,0,0,0.06),0 2px 2px 0 rgba(0,0,0,0.1);
box-shadow: 0 0 0 1px rgba(0,0,0,0.06), 0 2px 2px 0 rgba(0,0,0,0.1);
cursor: default;

}
input#password{
  width:100vw;
  max-width:260px;
  border:1px solid #cbcbcb;
  margin-bottom:15px;
}
#login_form .btn{
  background: {{settings.btn_1_bg_color}};
  color: {{settings.btn_1_text_color}};
  border-color: {{settings.btn_1_border_color}};
  padding: 10px 41px 8px;
  width:100%;
}
#login_form label{
  font-weight: 400;
}
#login_form .btn:hover{
  background: {{settings.btn_2_bg_color}};
  color: {{settings.btn_2_text_color}};
  border-color: {{settings.btn_2_border_color}};
}
@media (max-width:1024px){
  .header-password .header-logo img{
    max-height:{{section.settings.max_height_logo_mb}}px;
  }
  .header-password .header-logo img:first-child{
    display:none;
  }
  .header-password .header-logo img:last-child{
    display:block;
  }
  .password-content{
    flex-wrap: wrap;
    flex-direction: column-reverse;
  }
  .password-content .col{
    max-width: 100%;
  }

  .password-content .content{
    position:static;
    -webkit-transform: translate(0%,0%);
    -ms-transform: translate(0%,0%);
    transform: translate(0%,0%);
    text-align:center;
    margin:0 auto;
    padding:60px 30px 0px;
  }
  .header-password{
    padding:30px 15px;
  }
  footer{
    position:static;
    padding:60px 10px;
    text-align: center;
  }
}
</style>
{% else %}
